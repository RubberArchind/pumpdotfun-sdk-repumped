{
  "name": "pumpdotfun-repumped-sdk",
  "version": "1.5.0",
  "description": "Pumpfun SDK â€” create, buy, sell tokens with support for Jito bundles and multiple relayer integrations. Rebuilt and fixed pumpdotfun-sdk.",
  "author": "D3AD-E <evg.gordi@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/D3AD-E/pumpdotfun-sdk-repumped.git"
  },
  "bugs": {
    "url": "https://github.com/D3AD-E/pumpdotfun-sdk-repumped/issues"
  },
  "homepage": "https://github.com/D3AD-E/pumpdotfun-sdk-repumped#readme",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "rimraf dist && rollup -c --bundleConfigAsCjs",
    "prepare": "npm run build",
    "example": "npm run build && tsx example/index.ts",
    "lint": "tsc -p tsconfig.json --noEmit",
    "test": "npm run build && tsx test/integration.test.ts",
    "sell": "npm run build && tsx example/sellToken.ts",
    "test:sell": "npm run build && tsx example/testSell.ts",
    "fix:old-token": "npm run build && tsx example/fixOldToken.ts"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.98.2",
    "bn.js": "^5.2.2",
    "bs58": "^6.0.0",
    "jito-ts": "^4.2.0"
  },
  "devDependencies": {
    "@coral-xyz/borsh": "^0.31.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.3",
    "@tsconfig/node20": "^20.1.6",
    "@types/node": "^20.19.1",
    "dotenv": "^16.5.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.44.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "pump.fun",
    "pumpfun",
    "pumpdotfun",
    "solana",
    "pumpdotfunsdk",
    "sdk",
    "token",
